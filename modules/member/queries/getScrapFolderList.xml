<query id="getScrapFolderList" action="select">
    <tables>
        <table name="member_scrap_folders" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="member_srl" var="member_srl" filter="number" />
    </conditions>
    <navigation>
        <index var="sort_index" default="list_order" order="asc" />
    </navigation>
</query>
