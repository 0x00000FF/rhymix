<query id="updateMemberPhoneNumber" action="update">
    <tables>
        <table name="member" />
    </tables>
    <columns>
        <column name="phone_number" var="phone_number" notnull="notnull" />
    </columns>
    <conditions>
        <condition operation="equal" column="member_srl" var="member_srl" notnull="notnull" filter="number" />
    </conditions>
</query>
